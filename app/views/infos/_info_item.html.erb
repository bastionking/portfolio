<div class="col-md-4">

  <div class="card" data-id="<%= info_item.id %>">
   <%= image_tag portfolio_img(info_item.thumb_image, 'thumb'), width: '100%' %>
    <p class="card-text"
      <span><%= link_to info_item.title, info_show_path(info_item) %></span>
      <%= info_item.subtitle %>
    </p>
  </div>

</div>
